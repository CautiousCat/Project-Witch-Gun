config_version=4

[application]

config/name="Witch Gun"
run/main_scene="res://Scenes/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

PlayerStats="*res://Scripts/Player/PlayerStats.gd"
AudioManager="*res://Scenes/AudioManager.tscn"
GameManager="*res://Scripts/GameManager.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1280
window/size/test_height=720
mouse_cursor/custom_image="res://Graphics/Misc/CursorCrosshair.png"
mouse_cursor/custom_image_hotspot=Vector2( 16, 16 )
mouse_cursor/tooltip_position_offset=Vector2( 16, 16 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

frames=false

